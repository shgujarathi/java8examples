<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="details.MachineDetails" table="SRAW">
		<id name="ID" type="java.math.BigDecimal">
			<column name="id" not-null="true" />
			<generator class="identity" />
		</id>
		<property name="EMP_CODE" type="string">
			<column name="EMP_CODE" />
		</property>

		<property name="ATT_DATE" type="java.util.Date">
			<column name="ATT_DATE" />
		</property>

		<property name="IN_OUT" type="string">
			<column name="IN_OUT" />
		</property>

		<property name="CARDNO" type="string">
			<column name="CARDNO" />
		</property>

	</class>
</hibernate-mapping>
